{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "singularName": "event",
    "pluralName": "events",
    "displayName": "event"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "urlSlug": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "previewImage": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "name": {
      "type": "string",
      "required": true
    },
    "dateStart": {
      "type": "datetime",
      "required": true
    },
    "dateEnd": {
      "type": "datetime",
      "required": true
    },
    "descriptionShort": {
      "type": "string",
      "required": true
    },
    "descriptionFull": {
      "type": "blocks",
      "required": true
    },
    "location": {
      "type": "string",
      "required": true
    },
    "organizations": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::organization.organization",
      "mappedBy": "event"
    },
    "gallery": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::gallery.gallery",
      "inversedBy": "event"
    }
  }
}
