{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "singularName": "event",
    "pluralName": "events",
    "displayName": "event"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "UrlSlug": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "PreviewImage": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "Name": {
      "type": "string",
      "required": true
    },
    "DateStart": {
      "type": "datetime",
      "required": true
    },
    "DateEnd": {
      "type": "datetime",
      "required": true
    },
    "DescriptionShort": {
      "type": "string",
      "required": true
    },
    "DescriptionFull": {
      "type": "blocks",
      "required": true
    },
    "Location": {
      "type": "string",
      "required": true
    },
    "Organizations": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::organization.organization",
      "mappedBy": "Event"
    },
    "Gallery": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::gallery.gallery",
      "inversedBy": "event"
    }
  }
}
